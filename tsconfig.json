{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom", "es5", "es2015.collection", "es2015.iterable"],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
  },
  "include": [
    "src"
  ]
}
